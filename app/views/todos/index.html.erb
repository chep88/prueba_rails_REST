
<% @todos.each do |todo| %>
  <div class="well">
    <%= " #{todo.id}) Descripcion :   #{todo.description} -  Completado : #{todo.completed}"%>
    <p><%= link_to "Ver", todo, class: 'btn btn-success' %>  - <%= link_to "Borrar", todo, method: :delete, class: 'btn btn-danger' %></p>
  </div>
<% end %>
<%= link_to "Crear", todos_new_path, class: 'btn btn-info'%>
