<div class="col-lg-6">
  <% @false.each do |todo| %>
    <div class="col-lg-12">
      <div class="well">
        <%= " #{todo.id}) Descripcion :   #{todo.description} -  Completado : #{todo.completed}"%>
      </div>
    </div>
  <% end %>
</div>
<div class="col-lg-6">
  <% @true.each do |todo| %>
    <div class="col-lg-12">
      <div class="well">
        <%= " #{todo.id}) Descripcion :   #{todo.description} -  Completado : #{todo.completed}"%>
      </div>
    </div>
  <% end %>
</div>
